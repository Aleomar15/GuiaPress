<%- include ('../../partials/header.ejs');%>
<div class="container">
    <hr>
    <div class="card mt-4">
        <div class="card-header">
            <h2>Criação de usuario</h2>
        </div>
        <div class="card-body">
            <form action="/users/create" method="post">
                <input class="form-control" name="email" type="email" placeholder="emai@gmail.com"><br>
                <input class="form-control" name="password" type="password" placeholder="******"><br>
                <button type="submit" class="btn btn-primary btn-lg col-12">Criar</button>
                <a href="/login">Login</a>
            </form>
        </div>
    </div>
</div>
<%- include ('../../partials/footer.ejs');%>